{
  "name": "n8n-nodes-noopa",
  "version": "0.2.0",
  "description": "n8n nodes for Azure Document Intelligence and other integrations",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "beery",
    "email": "beeryba05@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noops-studio/n8n-nodes-noopa"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [
      "dist/credentials/AzureDocumentIntelligenceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Example/Example.node.js",
      "dist/nodes/AzureDocumentIntelligence/AzureDocumentIntelligence.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@azure-rest/ai-document-intelligence": "^1.1.0",
    "@azure/core-auth": "^1.6.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
